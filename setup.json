{
    "name": "aiida-bjm",
    "author": "Pezhman Zarabadi-Poor",
    "author_email": "pzp22@bath.ac.uk",
    "description": "Collection of AiiDA WorkChains Developed in Morgan Group",
    "url": "https://github.com/morgan-group-bath/aiida-bjm",
    "license": "MIT License",
    "classifiers": [
        "Programming Language :: Python :: 3.6",
        "Programming Language :: Python :: 3.7"
    ],
    "version": "0.3.0",
    "setup_requires": ["reentry"],
    "reentry_register": true,
    "install_requires": [
        "aiida-core[atomic_tools]>=1.1.0,!=1.4.0,!=1.4.1"
    ],
    "entry_points": {
        "aiida.parsers": [
            "vasp_base_parser = aiida_bjm.parsers:VaspBaseParser"
        ],
        "aiida.workflows": [
            "vasp.base = aiida_bjm.workchains:VaspBaseWorkChain",
            "bjm.vasp_multistage = aiida_bjm.workchains:VaspMultiStageWorkChain",
            "bjm.vasp_converge = aiida_bjm.workchains:VaspConvergeWorkChain",
            "bjm.vasp_catmat = aiida_bjm.workchains:VaspCatMatWorkChain",
            "bjm.vasp_multistage_ddec = aiida_bjm.workchains:VaspMultiStageDdecWorkChain"
        ]
    },
    "data_files": [
        [".", ["setup.json"]]
    ],
    "extras_require": {
        "testing": [
            "pytest==4.4.1"
        ],
        "pre-commit": [
            "pre-commit==2.0.1",
            "yapf==0.28.0",
            "prospector==1.2.0",
            "pylint==2.4.4"
        ]
    }
}
